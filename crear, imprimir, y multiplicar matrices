#include<iostream>
using namespace std;
int main()
{
    srand(1234);
    int n, m;
    cout<<"Ingrese el valor de n: ";
    cin>>n;
    cout<<"Ingrese el valor de m: ";
    cin>>m;
    cout<<"La matriz A tendr치 tama침o de "<<n<<" x "<<m<<endl;
    
    int a[n][m];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            a[i][j] = rand()%100;
        }
    }
    
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cout<<a[i][j]<<"\t";
        }
        cout<<endl;
    }
    
    
    int o, p;
    
    do{
        cout<<"Ingrese el valor de o: ";
        cin>>o;
    }while(o != m);
    
    cout<<"Ingrese el valor de p: ";
    cin>>p;
    cout<<"La matriz B tendr치 tama침o de "<<o<<" x "<<p<<endl;
    
    int b[o][p];
    for (int i = 0; i < o; i++) {
        for (int j = 0; j < p; j++) {
            b[i][j] = rand()%100;
        }
    }
    
    for (int i = 0; i < o; i++) {
        for (int j = 0; j < p; j++) {
            cout<<b[i][j]<<"\t";
        }
        cout<<endl;
    }
    
    int c[n][p];
    
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < p; j++) {
            c[i][j] = 0;
            for (int k = 0; k < m; k++) {
                c[i][j] += a[i][k]*b[k][j];
            }
        }
    }
    cout<<"La matriz c es: "<<endl;
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < p; j++) {
            cout<<c[i][j]<<"\t";
        }
        cout<<endl;
    }
    
    
    return 0;
}
