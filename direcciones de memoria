#include<iostream>

using namespace std;

int main()
{
    int a;
    cout<<"****A****"<<endl;
    cout<<a<<endl;
    cout<<&a<<endl;
    
    long v[5];
    for (int i = 0; i < 5; i++) {
        v[i] = 10+i;
    }
    
    cout<<"****V****"<<endl;
    cout<<v<<endl;
    cout<<v[0]<<endl;
    cout<<&v[0]<<endl;
    cout<<v[1]<<endl;
    cout<<&v[1]<<endl;
    cout<<v[4]<<endl;
    cout<<&v[4]<<endl;
    
    int m[3][3];
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            m[i][j] = i*3+j;
        }
    }
    cout<<"****M****"<<endl;
    cout<<m<<endl;
    cout<<m[0][0]<<endl;
    cout<<&m[0][0]<<endl;
    cout<<m[2][1]<<endl;
    cout<<&m[2][1]<<endl;
    
    
    
    return 0;
}
